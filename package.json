{
  "name": "termfeed",
  "version": "0.5.0",
  "description": "A terminal-based RSS reader with Vim-like keybindings",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "termfeed": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "migrate": "tsx src/apps/cli/migrate.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run test:run && npm run typecheck"
  },
  "keywords": [
    "rss",
    "feed",
    "reader",
    "terminal",
    "cli",
    "tui",
    "vim"
  ],
  "author": "syou6162 <syou6162@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syou6162/termfeed.git"
  },
  "bugs": {
    "url": "https://github.com/syou6162/termfeed/issues"
  },
  "homepage": "https://github.com/syou6162/termfeed#readme",
  "files": [
    "dist/",
    "src/models/schema.sql",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "@types/better-sqlite3": "^7.6.11",
    "@types/react": "^19.1.0",
    "axios": "^1.7.9",
    "better-sqlite3": "^12.0.0",
    "chalk": "^5.4.0",
    "commander": "^14.0.0",
    "ink": "^6.0.1",
    "ink-testing-library": "^4.0.0",
    "react": "^19.1.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.33",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "pre-commit": "^1.2.2",
    "prettier": "^3.6.2",
    "tsx": "^4.20.0",
    "typescript": "^5.8.3",
    "vitest": "^4.0.0"
  },
  "pre-commit": [
    "lint",
    "typecheck"
  ]
}
